%function sA = sortCellArray(cA, col)

%isolate the column to sort with
sortCol = cA(:,colm);

% retrieve nummerical values  (Strings may not work)
sortNumCol = zeros(size(sortCol));
for count = 1:max(size(sortCol))
    sortNumCol(count) = sortCol{count};
end

% sort numbers
[sortNumCol indexAray] = sort(sortNumCol);

% sort the Cell array
for iCount = 1:size(cA,2)
    for jCount = 1:size(cA,1)
        
        
    end
end
sA(:,indexArray) = cA(:,indexArray)

%end
